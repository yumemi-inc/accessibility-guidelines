{
  "name": "accessibility-guidelines",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint . --fix",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint:textlint": "textlint \"src/**/*.{md,mdx}\"",
    "fix:textlint": "textlint --fix \"src/**/*.{md,mdx}\"",
    "lint:cspell": "cspell --config cspell.json \"**/*.{ts,tsx,js,jsx,md,mdx,json}\"",
    "fix:cspell": "pnpm run lint:cspell",
    "lint": "concurrently pnpm:lint:*",
    "fix": "concurrently pnpm:fix:*"
  },
  "dependencies": {
    "@astrojs/starlight": "0.35.2",
    "@fontsource/noto-sans-jp": "5.2.7",
    "astro": "5.13.7",
    "sharp": "0.34.3"
  },
  "devDependencies": {
    "@cspell/dict-css": "4.0.18",
    "@cspell/dict-en-common-misspellings": "2.1.6",
    "@cspell/dict-fullstack": "3.2.7",
    "@cspell/dict-html": "4.0.12",
    "@cspell/dict-npm": "5.2.17",
    "@cspell/dict-software-terms": "5.1.8",
    "@eslint/js": "9.35.0",
    "concurrently": "9.2.1",
    "cspell": "9.2.1",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.15.0",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "textlint": "15.2.2",
    "textlint-plugin-mdx": "1.0.2",
    "textlint-rule-preset-ja-technical-writing": "12.0.2",
    "textlint-rule-preset-jtf-style": "3.0.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
  },
  "packageManager": "pnpm@10.15.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
