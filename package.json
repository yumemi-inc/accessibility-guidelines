{
  "name": "accessibility-guidelines",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint . --fix",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint:textlint": "textlint \"src/**/*.{md,mdx}\"",
    "fix:textlint": "textlint --fix \"src/**/*.{md,mdx}\"",
    "lint:cspell": "cspell --config cspell.json \"**/*.{ts,tsx,js,jsx,md,mdx,json}\"",
    "fix:cspell": "pnpm run lint:cspell",
    "lint": "concurrently pnpm:lint:*",
    "fix": "concurrently pnpm:fix:*"
  },
  "dependencies": {
    "@astrojs/starlight": "0.34.3",
    "@fontsource/noto-sans-jp": "5.2.5",
    "astro": "5.8.1",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@cspell/dict-css": "4.0.17",
    "@cspell/dict-en-common-misspellings": "2.0.11",
    "@cspell/dict-fullstack": "3.2.6",
    "@cspell/dict-html": "4.0.11",
    "@cspell/dict-npm": "5.2.4",
    "@cspell/dict-software-terms": "5.0.10",
    "@eslint/js": "9.27.0",
    "concurrently": "9.1.2",
    "cspell": "9.0.2",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "4.13.0",
    "globals": "16.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "textlint": "15.2.2",
    "textlint-plugin-mdx": "1.0.2",
    "textlint-rule-preset-ja-technical-writing": "12.0.2",
    "textlint-rule-preset-jtf-style": "3.0.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.0"
  },
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
